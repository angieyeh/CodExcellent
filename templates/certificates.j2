  <div id="browse">
  <table border="1" cellpadding="5">
    <tr>
      <th>certificate_id</th>
      <th>certificate_title</th>
      <th>issue_date</th>
    </tr>
    <tr>
      <td>1</td>
      <td>Certificate of Completion</td>
      <td>2022-03-17</td>
    </tr>
    <tr>
      <td>2</td>
      <td>Certificate of Completion</td>
      <td>2022-03-17</td>
    </tr>
    <tr>
      <td>3</td>
      <td>Certificate of Completion</td>
      <td>2022-03-17</td>
    </tr>
  </table>
  <p>&nbsp;</p>
  </div> <!-- browse -->
  <div id="insert">
      <form method="POST">
          <legend><strong>Add Certificate</strong></legend>
            <fieldset class="fields">
              <label>Certificate Title</label> <input type="text" name="certificate_title">
              <label>Issue Date</label> <input type="text" name="issue_date">  
            </fieldset>
            <input class="btn" type="submit" value="Add Certificate">
            <input class="btn" type="button" value="cancel">
    </form> 
  </div><!-- insert -->
  <p>&nbsp;</p>
  <div id="update">
      <form method="POST">
          <legend><strong>Update Certificate</strong></legend>
            <fieldset class="fields">
              <label>Certificate Title</label> <input type="text" name="certificate_title">  
              <label>Issue Date</label> <input type="text" name="issue_date">  
            </fieldset>
            <input class="btn" type="submit" value="Update Certificate">
            <input class="btn" type="button" value="cancel">
    </form> 
  </div><!-- update -->
  <p>&nbsp;</p>
  <div id="delete" style="display: block">
    <form method="POST">
        <legend><strong>Delete Certificate</strong></legend>
            <fieldset class="fields">
            <p>Are you sure you wish to delete the following?</p>
            <input type="hidden" name="certificate_id" value="1">
            <label>Certificate ID</label> <input type="text" name="certificate_title">  
              
      </fieldset>
      <input class="btn" type="submit" value="Delete Certificate">
      <input class="btn" type="button" value="cancel">
    </form> 
  </div><!-- delete -->
